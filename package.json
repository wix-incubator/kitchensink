{
  "name": "kitchensink",
  "type": "module",
  "version": "0.0.1",
  "scripts": {
    "unimported": "unimported --fix",
    "dev": "wix dev",
    "build": "wix build",
    "build:components": "tsup",
    "preview": "wix preview",
    "release": "wix release",
    "astro": "astro",
    "clean": "find . -name 'node_modules' -type d -prune -exec rm -rf '{}' + && rm -rf .astro ./dist",
    "test": "npm run test:playwright",
    "test:playwright": "playwright test",
    "test:ui": "playwright test --ui",
    "test:headed": "playwright test --headed",
    "test:debug": "playwright test --debug",
    "test:install": "playwright install chromium",
    "format": "prettier --write src/",
    "format:check": "prettier --check src/",
    "lint": "npm run format:check && astro check",
    "knip": "knip"
  },
  "dependencies": {
    "@astrojs/check": "^0.9.4",
    "@astrojs/react": "^4.2.7",
    "@astrojs/sitemap": "^3.4.1",
    "@radix-ui/react-icons": "^1.3.2",
    "@radix-ui/react-label": "^2.1.6",
    "@radix-ui/react-select": "^2.2.4",
    "@radix-ui/react-slot": "^1.2.2",
    "@radix-ui/react-tooltip": "^1.2.6",
    "@types/react": "^18.3.1",
    "@types/react-dom": "^18.3.1",
    "@wix/astro": "^1.0.22",
    "@wix/dashboard": "^1.3.39",
    "@wix/ecom": "^1.0.1376",
    "@wix/essentials": "^0.1.24",
    "@wix/headless-components": "^0.0.12",
    "@wix/sdk": "^1.15.23",
    "@wix/stores": "^1.0.512",
    "astro": "^5.9.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-share": "^5.2.2",
    "tailwind-merge": "^3.3.1"
  },
  "devDependencies": {
    "@astrojs/cloudflare": "^12.5.4",
    "@astrojs/mdx": "^4.3.0",
    "@playwright/test": "^1.53.2",
    "@tailwindcss/postcss": "^4.1.12",
    "@tailwindcss/vite": "^4.0.13",
    "@types/node": "^24.4.0",
    "@wix/cli": "^1.1.95",
    "knip": "^5.63.1",
    "postcss": "^8.5.6",
    "prettier": "^3.6.2",
    "prettier-plugin-astro": "^0.14.1",
    "tsup": "^8.5.0",
    "typescript": "^5.9.2",
    "unimported": "^1.31.1"
  },
  "files": [
    "src",
    "dist"
  ],
  "exports": {
    "./StoreLayout": {
      "types": "./dist/components/store/StoreLayout.d.ts",
      "import": "./dist/components/store/StoreLayout.js",
      "require": "./dist/components/store/StoreLayout.js"
    },
    "./ProductList": {
      "types": "./dist/components/store/ProductList.d.ts",
      "import": "./dist/components/store/ProductList.js",
      "require": "./dist/components/store/ProductList.js"
    },
    "./ProductList.css": "./dist/ProductList.css",
    "./ProductDetails": {
      "types": "./dist/components/store/ProductDetails.d.ts",
      "import": "./dist/components/store/ProductDetails.js",
      "require": "./dist/components/store/ProductDetails.js"
    },
    "./ProductDetails.css": "./dist/ProductDetails.css",
    "./styles.css": "./dist/styles.css"
  }
}
