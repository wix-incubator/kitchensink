---
import BaseLayout from '../../../layouts/BaseLayout.astro';
import BookingsServicesPage from '../../../react-pages/bookings/example-2/services.tsx';
import { loadBookingServicesServiceConfig } from '../../../headless/bookings/services/booking-services-service';

// Load all services
const bookingServicesConfig = await loadBookingServicesServiceConfig();
---

<BaseLayout>
  <title>All Services - Book Your Coaching Session</title>
  <meta
    name="description"
    content="Browse all our personal coaching services and book the one that fits your needs."
  />

  <BookingsServicesPage
    client:load
    bookingServicesConfig={bookingServicesConfig}
    slot="body"
  />
</BaseLayout>
